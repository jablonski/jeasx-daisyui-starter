{
  "name": "jeasx-daisyui-starter",
  "type": "module",
  "scripts": {
    "start": "jeasx start",
    "build": "jeasx build",
    "dev": "concurrently -c auto -n dev,css 'jeasx dev' 'npm:css -- --watch'",
    "css": "npx @tailwindcss/cli -i ./src/browser/tailwind/source.css -o ./src/browser/tailwind/output.css"
  },
  "browserslist": "defaults, not ie <= 11",
  "dependencies": {
    "@tailwindcss/cli": "4.1.13",
    "concurrently": "9.2.1",
    "daisyui": "5.1.9",
    "jeasx": "1.8.5",
    "tailwindcss": "4.1.13"
  },
  "devDependencies": {
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14"
  }
}
